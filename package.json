{
  "name": "js13kserver",
  "version": "1.0.2",
  "description": "js13kgames game jam server",
  "license": "MIT",
  "main": "index.js",
  "private": "true",
  "scripts": {
    "start": "node index.js",
    "start:dev": "concurrently 'nodemon' && 'npm run build:dev'",
    "test": "mocha test",
    "format": "prettier --write public/",
    "build": "rollup -c --compact",
    "build:dev": "rollup -c -w --compact"
  },
  "keywords": [
    "js13kgames",
    "server"
  ],
  "engines": {
    "node": "8.*"
  },
  "nodemonConfig": {
    "ext": "js,mjs,ts,json,css,scss,sass,html,pug"
  },
  "dependencies": {
    "archiver": "^4.0.2",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mysql2": "^2.1.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.3",
    "socket.io": "^2.3.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^5.0.2",
    "concurrently": "^5.3.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rollup": "^2.26.4",
    "rollup-plugin-terser": "^7.0.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  }
}
